{
  "name": "refactor-css",
  "displayName": "Refactor CSS",
  "description": "Helps you identify reoccurring CSS class name combinations in your markup.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "keywords": [
    "css",
    "refactor",
    "utility first css",
    "class",
    "classes"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:php",
    "onLanguage:js"
  ],
  "main": "./out/extension",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "prettier": "^1.15.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "license": "MIT",
  "publisher": "urbantrout",
  "author": {
    "name": "Harald Atteneder",
    "email": "harald@urbantrout.io",
    "url": "https://urbantrout.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urbantrout/refactor-css.git"
  }
}
